              {
                "$ref": "#/components/schemas/USAccountSettings"
              },
              {
                "$ref": "#/components/schemas/CanadaAccountSettings"
              }
            ]
          }
        }
      },
      "CanadaAccountSettings": {
        "title": "CanadaAccountSettings",
        "required": [
          "balance",
          "country",
          "state",
          "visa_debit_enabled"
        ],
        "type": "object",
        "properties": {
          "balance": {
            "title": "Balance",
            "type": "number"
          },
          "country": {
            "title": "Country",
            "type": "string"
          },
          "state": {
            "title": "State",
            "anyOf": [
              {
                "const": "ON",
                "type": "string"
              },
              {
                "const": "QC",
                "type": "string"
              },
              {
                "const": "BC",
                "type": "string"
              }
            ]
          },
          "visa_debit_enabled": {
            "title": "Visa Debit Enabled",
            "type": "boolean"
          }
        }
      },
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      },
      "USAccountSettings": {
        "title": "USAccountSettings",
        "required": [
          "balance",
          "country",
          "state",
          "ach_enabled"
        ],
        "type": "object",
        "properties": {
          "balance": {
            "title": "Balance",
            "type": "number"
          },
          "country": {
            "title": "Country",
            "type": "string"
          },
          "state": {
            "title": "State",
            "anyOf": [
              {
                "const": "NY",
                "type": "string"
              },
              {
                "const": "NJ",
                "type": "string"
              },
              {
                "const": "WA",
                "type": "string"
              }
            ]
          },
          "ach_enabled": {
            "title": "Ach Enabled",
            "type": "boolean"
          }
        }
      },
      "ValidationError": {
        "title": "ValidationError",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "type": "object",
        "properties": {
          "loc": {
            "title": "Location",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        }
      }
